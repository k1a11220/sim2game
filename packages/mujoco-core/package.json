{
  "name": "@sim2game/mujoco-core",
  "version": "0.0.0",
  "description": "MuJoCo WebAssembly runtime wrapper utilities",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./worker": {
      "types": "./dist/worker/client.d.ts",
      "import": "./dist/worker/client.js"
    },
    "./worker/entry": {
      "types": "./dist/worker/entry.d.ts",
      "import": "./dist/worker/entry.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "sync:wasm": "node ./scripts/sync-wasm.mjs",
    "build": "npm run sync:wasm && tsc -p tsconfig.json",
    "lint": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "typescript": "^5.9.2"
  }
}
